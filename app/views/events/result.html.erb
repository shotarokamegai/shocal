<% events.each do |event| %>
<div class="event-result row">
	<div class="col-md-3" id="one">
		<h3><%= event["name"]["text"] %></h3>
		<img src="<%= event["logo_url"] %>" class="img-thumbnail" style="width: 90px; height: 90px" style="display: inline;">
	</div>
	<div class="col-md-3" id="two">
		<div class="row">
			<strong>date:</strong><br />
			<h4><% time = event["start"]["local"] %>
				<% year = time.split('-')[0] %>
				<% month = time.split('-')[1] %>
				<% yo = time.split('-')[2] %>
				<% date = yo.split('T')[0] %>
				<% time = yo.split('T')[1] %>
				<%= year %>/<%= month %>/<%= date %> <%= time %></h4>
		</div>	
		<div class="row">
			<strong>address:</strong><br /> 
			<h4><%= event["venue"]["address"]["address_1"] %>, 
				<%= event["venue"]["address"]["city"] %>,
				<%= event["venue"]["address"]["region"] %>, 
				<%= event["venue"]["address"]["postal_code"]%>, 
				<%= event["venue"]["address"]["country"]%>
			</h4>
		</div>
	</div>
	<div class="col-md-3" id="three">
		<div class="row">
			<p>
			<strong>description:</strong> <%= event["description"]["text"] %>
			</p>
			<p><a href="<%= event["url"] %>"><%= event["url"] %></a></p>
		</div>
		<div class="button-option row">
			<button type="button" class="btn btn-info">add event</button>&nbsp;&nbsp;
			<button type="button" class="btn btn-info">share</button>
		</div>	
	</div>
	<div class="col-md-3" id="four">
		<div id="map-canvas" style="width: 50%; height: 50%"></div>
	</div>
</div>
<hr />
<% end %>